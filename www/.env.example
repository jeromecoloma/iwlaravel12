SITE_CONTACT_EMAIL="hello@iwlaravel12.com"
SITE_SUPPORT_EMAIL="support@iwlaravel12.com"

# Social Media
SITE_TWITTER_HANDLE="@iwlaravel12"
SITE_GITHUB_URL="https://github.com/iwlaravel12"

# SEO Settings
SITE_SEO_TITLE=iwlaravel12
SITE_SEO_TITLE_SEPARATOR=" | "
SITE_SEO_DESCRIPTION="Modern Laravel 12 starter template with React, Inertia.js, and shadcn/ui"
